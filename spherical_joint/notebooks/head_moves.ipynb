    "t=0\n",
    "while t < 2.5:\n",
    "    angle1=0.5*math.sin(20*t)\n",
    "    angle2=0.5*math.sin(10*t)\n",
    "    q11,q12,q13 = a.get_angles_from_vector([0.2+angle1,angle2,1],0)\n",
