    "t=0\n",
    "while t < 2.5:\n",
    "    angle=10*math.sin(5*t)\n",
    "    q11,q12,q13 = a.get_angles_from_vector([0.3,0,1],angle)\n",
    "    r.disk_top.target_rot_position = q11\n",
    "    r.disk_middle.target_rot_position = q12\n",
    "    r.disk_bottom.target_rot_position = q13\n",
    "    time.sleep(0.01)\n",
    "    t=t+0.01"
   ]
